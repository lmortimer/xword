        "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^6.3.2",
        "core-js": "^3.3.6",
        "css-loader": "^5.0.1",
        "dotenv-webpack": "^6.0.0",
        "esm": "^3.2.25",
        "file-loader": "^6.2.0",
